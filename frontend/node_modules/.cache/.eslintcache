tor((state) => state.userSignin);\n  const { userInfo } = userSignin;\n\n  const productReviewCreate = useSelector((state) => state.productReviewCreate);\n  const {\n    loading: loadingReviewCreate,\n    error: errorReviewCreate,\n    success: successReviewCreate,\n  } = productReviewCreate;\n\n  const [rating, setRating] = useState(0);\n  const [comment, setComment] = useState('');\n\n  useEffect(() => {\n    if (successReviewCreate) {\n      window.alert('Review Submitted Successfully');\n      setRating('');\n      setComment('');\n      dispatch({ type: PRODUCT_REVIEW_CREATE_RESET });\n    }\n    dispatch(detailsProduct(productId));\n  }, [dispatch, productId, successReviewCreate]);\n  const addToCartHandler = () => {\n    navigate(`/cart/${productId}?qty=${qty}`);\n  };\n  const submitHandler = (e) => {\n    e.preventDefault();\n    if (comment && rating) {\n      dispatch(\n        createReview(productId, { rating, comment, name: userInfo.name })\n      );\n    } else {\n      alert('Please enter comment and rating');\n    }\n  };\n  return (\n    <div>\n      {loading ? (\n        <LoadingBox></LoadingBox>\n      ) : error ? (\n        <MessageBox variant=\"danger\">{error}</MessageBox>\n      ) : (\n        <div>\n          <Link to=\"/\"> Volver </Link>\n          <div className=\"row top\">\n            <div className=\"col-2\">\n              <img\n                className=\"large\"\n                src={product.image}\n                alt={product.name}\n              ></img>\n            </div>\n            <div className=\"col-1\">\n              <ul>\n                <li>\n                  <h1>{product.name}</h1>\n                </li>\n                \n                <li>Precio: ${product.price}</li>\n                <li>\n                  Descripción:\n                  <p>{product.description}</p>\n                </li>\n              </ul>\n            </div>\n            <div className=\"col-1\">\n              <div className=\"card card-body-cart\">\n                <ul>\n                 \n                  <li>\n                    <div className=\"row\">\n                      <div>Precio</div>\n                      <div className=\"price\">${product.price}</div>\n                    </div>\n                  </li>\n                  <li>\n                    <div className=\"row\">\n                      <div>Estado</div>\n                      <div>\n                        {product.countInStock > 0 ? (\n                          <span className=\"success\">Disponible</span>\n                        ) : (\n                          <span className=\"danger\">No Disponible</span>\n                        )}\n                      </div>\n                    </div>\n                  </li>\n                  {product.countInStock > 0 && (\n                    <>\n                      <li>\n                        <div className=\"row\">\n                          <div>Cantidad</div>\n                          <div>\n                            <select\n                              value={qty}\n                              onChange={(e) => setQty(e.target.value)}\n                            >\n                              {[...Array(product.countInStock).keys()].map(\n                                (x) => (\n                                  <option key={x + 1} value={x + 1}>\n                                    {x + 1}\n                                  </option>\n                                )\n                              )}\n                            </select>\n                          </div>\n                        </div>\n                      </li>\n                      <li>\n                        <button\n                          onClick={addToCartHandler}\n                          className=\"primary block\"\n                        >\n                          Agregar al carrito\n                        </button>\n                      </li>\n                    </>\n                  )}\n                </ul>\n              </div>\n            </div>\n          </div>\n          \n                 \n              \n          \n          </div>\n        \n      )}\n    </div>\n  );\n}\n","/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/ProfileScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/RegisterScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/ShippingAddressScreen.js",["215"],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/SigninScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/ProductEditScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/UserListScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/OrderListScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/UserEditScreen.js",["216","217"],"import React from 'react';\nimport { useEffect } from 'react';\nimport { useState } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { useNavigate, useParams } from 'react-router-dom';\nimport { detailsUser, updateUser } from '../actions/userActions';\nimport LoadingBox from '../components/LoadingBox';\nimport MessageBox from '../components/MessageBox';\nimport { USER_UPDATE_RESET } from '../constants/userConstants';\n\nexport default function UserEditScreen(props) {\n  const navigate = useNavigate();\n  const params = useParams();\n  const { id: userId } = params;\n\n  const [username, setUsername] = useState('');\n  const [email, setEmail] = useState('');\n  const [isSeller, setIsSeller] = useState(false);\n  const [isAdmin, setIsAdmin] = useState(false);\n\n  const userDetails = useSelector((state) => state.userDetails);\n  const { loading, error, user } = userDetails;\n\n  const userUpdate = useSelector((state) => state.userUpdate);\n  const {\n    loading: loadingUpdate,\n    error: errorUpdate,\n    success: successUpdate,\n  } = userUpdate;\n\n  const dispatch = useDispatch();\n  useEffect(() => {\n    if (successUpdate) {\n      dispatch({ type: USER_UPDATE_RESET });\n      navigate('/userlist');\n    }\n    if (!user) {\n      dispatch(detailsUser(userId));\n    } else {\n      setUsername(user.username);\n      setEmail(user.email);\n      setIsAdmin(user.isAdmin);\n    }\n  }, [dispatch, navigate, successUpdate, user, userId]);\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n    // dispatch update user\n    dispatch(updateUser({ id: userId, username, email, isAdmin }));\n  };\n  \n  return (\n    <div>\n      <form className=\"form\" onSubmit={submitHandler}>\n        <div>\n          <h1>Editar Usuario {username}</h1>\n          {loadingUpdate && <LoadingBox></LoadingBox>}\n          {errorUpdate && (\n            <MessageBox variant=\"danger\">{errorUpdate}</MessageBox>\n          )}\n        </div>\n        {loading ? (\n          <LoadingBox />\n        ) : error ? (\n          <MessageBox variant=\"danger\">{error}</MessageBox>\n        ) : (\n          <>\n            <div>\n              <label htmlFor=\"username\">Nombre</label>\n              <input\n                id=\"username\"\n                type=\"text\"\n                placeholder=\"Enter name\"\n                value={username}\n                onChange={(e) => setUsername(e.target.value)}\n              ></input>\n            </div>\n            <div>\n              <label htmlFor=\"email\">Correo electrónico</label>\n              <input\n                id=\"email\"\n                type=\"email\"\n                placeholder=\"Enter email\"\n                value={email}\n                onChange={(e) => setEmail(e.target.value)}\n              ></input>\n            </div>\n           \n            <div>\n              <label htmlFor=\"isAdmin\">Es Administrador</label>\n              <input\n                id=\"isAdmin\"\n                type=\"checkbox\"\n                checked={isAdmin}\n                onChange={(e) => setIsAdmin(e.target.checked)}\n              ></input>\n            </div>\n            <div>\n              <button type=\"submit\" className=\"primary\">\n                Actualizar\n              </button>\n            </div>\n          </>\n        )}\n      </form>\n    </div>\n  );\n}\n","/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/DashboardScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/MapScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/SupportScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/SearchScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/reducers/cartReducers.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/screens/SellerScreen.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/reducers/orderReducers.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/reducers/productReducers.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/reducers/userReducers.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/actions/cartActions.js",["218","219"],"import Axios from 'axios';\nimport {\n  CART_ADD_ITEM,\n  CART_REMOVE_ITEM,\n  CART_SAVE_SHIPPING_ADDRESS,\n  CART_SAVE_PAYMENT_METHOD,\n  CART_ADD_ITEM_FAIL,\n} from '../constants/cartConstants';\n\nexport const addToCart = (productId, qty) => async (dispatch, getState) => {\n  const { data } = await Axios.get(`/api/products/${productId}`);\n  const {\n    cart: { cartItems },\n  } = getState();\n  \n    dispatch({\n      type: CART_ADD_ITEM,\n      payload: {\n        name: data.name,\n        image: data.image,\n        price: data.price,\n        countInStock: data.countInStock,\n        product: data.id,\n        qty,\n      },\n    });\n    localStorage.setItem(\n      'cartItems',\n      JSON.stringify(getState().cart.cartItems)\n    );\n  \n};\n\nexport const removeFromCart = (productId) => (dispatch, getState) => {\n  dispatch({ type: CART_REMOVE_ITEM, payload: productId });\n  localStorage.setItem('cartItems', JSON.stringify(getState().cart.cartItems));\n};\n\nexport const saveShippingAddress = (data) => (dispatch) => {\n  dispatch({ type: CART_SAVE_SHIPPING_ADDRESS, payload: data });\n  localStorage.setItem('shippingAddress', JSON.stringify(data));\n};\nexport const savePaymentMethod = (data) => (dispatch) => {\n  dispatch({ type: CART_SAVE_PAYMENT_METHOD, payload: data });\n};\n","/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/components/CheckoutSteps.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/components/Product.js",["220"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport Rating from './Rating';\n\nexport default function Product(props) {\n  const { product } = props;\n  return (\n    <div key={product.id} className=\"card\">\n      <Link to={`/product/${product.id}`}>\n        <img className=\"medium\" src={product.image} alt={product.name} />\n      </Link>\n      <div className=\"card-body\">\n        <Link to={`/product/${product.id}`}>\n          <h2>{product.name}</h2>\n        </Link>\n        <div className=\"row\">\n          <div className=\"price\">${product.price}</div>\n          \n        </div>\n      </div>\n    </div>\n  );\n}\n","/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/actions/orderActions.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/components/Rating.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/utils.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/constants/productConstants.js",[],"/home/francisco/webdev/ProyectoPasteleriaValdivia/frontend/src/constants/orderConstants.js",[],"/home/francisco/webdev/ProyectoPa